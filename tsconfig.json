{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "out",
        "rootDir": "src",
        "allowJs": true,
        "sourceMap": true,
        "noImplicitAny": false,
        "lib": [
            "es2016"
        ]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "typings"
    ]
}